    "@types/jasmine": "~5.1.13",
    "@types/three": "^0.181.0",
    "jasmine-core": "~5.12.1",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^21.0.0",
    "typescript": "~5.9.2"
  },
  "optionalDependencies": {
