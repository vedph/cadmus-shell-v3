{
  "name": "cadmus-shell",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-cor": "ng build @myrmidon/cadmus-core",
    "build-api": "ng build @myrmidon/cadmus-api",
    "build-ui": "ng build @myrmidon/cadmus-ui && ng build @myrmidon/cadmus-state",
    "build-uip": "ng build @myrmidon/cadmus-ui-pg",
    "build-ths": "ng build @myrmidon/cadmus-profile-core && ng build @myrmidon/cadmus-thesaurus-ui && ng build @myrmidon/cadmus-thesaurus-editor && ng build @myrmidon/cadmus-thesaurus-list",
    "build-gen": "ng build @myrmidon/cadmus-part-general-ui && ng build @myrmidon/cadmus-part-general-pg",
    "build-phi": "ng build @myrmidon/cadmus-part-philology-ui && ng build @myrmidon/cadmus-part-philology-pg",
    "build-itm": "ng build @myrmidon/cadmus-item-list && ng build @myrmidon/cadmus-item-editor && ng build @myrmidon/cadmus-item-search && ng build @myrmidon/cadmus-layer-demo",
    "build-grp": "ng build @myrmidon/cadmus-graph-ui && ng build @myrmidon/cadmus-graph-ui-ex && ng build @myrmidon/cadmus-graph-pg && ng build @myrmidon/cadmus-graph-pg-ex",
    "build-pre": "ng build @myrmidon/cadmus-preview-ui && ng build @myrmidon/cadmus-preview-pg",
    "build-flg": "ng build @myrmidon/cadmus-flags-ui && ng build @myrmidon/cadmus-flags-pg",
    "build-lib": "npm run build-cor && npm run build-api && npm run build-ui && npm run build-itm && npm run build-uip && npm run build-ths && npm run build-gen && npm run build-phi && npm run build-grp && npm run build-pre && npm run build-flg"
  },
  "overrides": {
    "d3-selection": "3.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.0",
    "@angular/cdk": "^18.1.0",
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/material": "^18.1.0",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/router": "^18.1.0",
    "@auth0/angular-jwt": "^5.2.0",
    "@cisstech/nge": "^17.7.1",
    "@myrmidon/auth-jwt-admin": "5.1.0",
    "@myrmidon/auth-jwt-login": "5.1.0",
    "@myrmidon/cadmus-mat-physical-state": "^0.0.2",
    "@myrmidon/cadmus-refs-asserted-chronotope": "5.1.3",
    "@myrmidon/cadmus-refs-asserted-ids": "^5.1.6",
    "@myrmidon/cadmus-refs-assertion": "5.1.1",
    "@myrmidon/cadmus-refs-doc-references": "^5.1.1",
    "@myrmidon/cadmus-refs-geonames-lookup": "^5.1.1",
    "@myrmidon/cadmus-refs-historical-date": "5.1.2",
    "@myrmidon/cadmus-refs-lookup": "^5.1.2",
    "@myrmidon/cadmus-refs-proper-name": "5.1.3",
    "@myrmidon/cadmus-refs-viaf-lookup": "^5.1.0",
    "@myrmidon/cadmus-text-block-view": "^5.1.0",
    "@myrmidon/cadmus-text-ed": "^5.1.0",
    "@myrmidon/cadmus-text-ed-md": "^5.1.2",
    "@myrmidon/cadmus-text-ed-txt": "^5.1.0",
    "@myrmidon/ng-mat-tools": "3.1.0",
    "@myrmidon/ng-tools": "3.1.1",
    "@myrmidon/paged-data-browsers": "^1.1.0",
    "@ngneat/effects-ng": "^3.1.4",
    "@swimlane/ngx-graph": "^8.4.0",
    "@types/diff-match-patch": "^1.0.36",
    "diff-match-patch": "^1.0.5",
    "fast-deep-equal": "^3.1.3",
    "marked": "^13.0.2",
    "monaco-editor": "^0.49.0",
    "rxjs": "~7.8.1",
    "ts-md5": "^1.3.1",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.0",
    "@angular/cli": "^18.1.0",
    "@angular/compiler-cli": "^18.1.0",
    "@angular/localize": "^18.1.0",
    "@types/d3-dispatch": "^3.0.6",
    "@types/d3-drag": "^3.0.7",
    "@types/d3-timer": "^3.0.2",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.14.11",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.1.0",
    "typescript": "~5.5.3"
  }
}
