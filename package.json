    "@myrmidon/cadmus-refs-asserted-chronotope": "10.0.1",
    "@myrmidon/cadmus-refs-asserted-ids": "^10.0.4",
    "@myrmidon/cadmus-refs-assertion": "10.0.1",
    "@myrmidon/cadmus-refs-citation": "^1.0.0",
    "@myrmidon/cadmus-refs-decorated-counts": "^9.0.0",
