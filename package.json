{
  "name": "cadmus-shell-v3",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-cor": "ng build @myrmidon/cadmus-core",
    "build-api": "ng build @myrmidon/cadmus-api",
    "build-ui": "ng build @myrmidon/cadmus-ui && ng build @myrmidon/cadmus-state",
    "build-uip": "ng build @myrmidon/cadmus-ui-pg",
    "build-ths": "ng build @myrmidon/cadmus-profile-core && ng build @myrmidon/cadmus-thesaurus-ui && ng build @myrmidon/cadmus-thesaurus-editor && ng build @myrmidon/cadmus-thesaurus-list",
    "build-gen": "ng build @myrmidon/cadmus-part-general-ui && ng build @myrmidon/cadmus-part-general-pg",
    "build-phi": "ng build @myrmidon/cadmus-part-philology-ui && ng build @myrmidon/cadmus-part-philology-pg",
    "build-itm": "ng build @myrmidon/cadmus-item-list && ng build @myrmidon/cadmus-item-editor && ng build @myrmidon/cadmus-item-search && ng build @myrmidon/cadmus-layer-demo",
    "build-grp": "ng build @myrmidon/cadmus-graph-ui && ng build @myrmidon/cadmus-graph-ui-ex && ng build @myrmidon/cadmus-graph-pg && ng build @myrmidon/cadmus-graph-pg-ex",
    "build-pre": "ng build @myrmidon/cadmus-preview-ui && ng build @myrmidon/cadmus-preview-pg",
    "build-flg": "ng build @myrmidon/cadmus-flags-ui && ng build @myrmidon/cadmus-flags-pg",
    "build-lib": "npm run build-cor && npm run build-api && npm run build-ui && npm run build-itm && npm run build-uip && npm run build-ths && npm run build-gen && npm run build-phi && npm run build-grp && npm run build-pre && npm run build-flg"
  },
  "overrides": {
    "d3-selection": "3.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.1",
    "@angular/cdk": "^19.2.2",
    "@angular/common": "^19.2.1",
    "@angular/compiler": "^19.2.1",
    "@angular/core": "^19.2.1",
    "@angular/forms": "^19.2.1",
    "@angular/material": "^19.2.2",
    "@angular/platform-browser": "^19.2.1",
    "@angular/platform-browser-dynamic": "^19.2.1",
    "@angular/router": "^19.2.1",
    "@auth0/angular-jwt": "^5.2.0",
    "@cisstech/nge": "^18.2.1",
    "@myrmidon/auth-jwt-admin": "7.1.1",
    "@myrmidon/auth-jwt-login": "7.1.1",
    "@myrmidon/cadmus-mat-physical-size": "^8.0.1",
    "@myrmidon/cadmus-mat-physical-state": "^8.0.3",
    "@myrmidon/cadmus-refs-asserted-chronotope": "9.0.0",
    "@myrmidon/cadmus-refs-asserted-ids": "^9.0.0",
    "@myrmidon/cadmus-refs-assertion": "9.0.0",
    "@myrmidon/cadmus-refs-decorated-counts": "^8.0.1",
    "@myrmidon/cadmus-refs-doc-references": "^9.0.0",
    "@myrmidon/cadmus-refs-geonames-lookup": "^8.0.0",
    "@myrmidon/cadmus-refs-historical-date": "8.0.0",
    "@myrmidon/cadmus-refs-lookup": "^8.0.1",
    "@myrmidon/cadmus-refs-proper-name": "9.0.0",
    "@myrmidon/cadmus-refs-viaf-lookup": "^8.0.0",
    "@myrmidon/cadmus-text-block-view": "^8.0.0",
    "@myrmidon/cadmus-text-ed": "^8.0.0",
    "@myrmidon/cadmus-text-ed-md": "^9.0.0",
    "@myrmidon/cadmus-text-ed-txt": "^8.0.0",
    "@myrmidon/ngx-mat-tools": "^0.0.1",
    "@myrmidon/ngx-tools": "^1.0.0",
    "@myrmidon/paged-data-browsers": "^4.0.2",
    "@swimlane/ngx-graph": "^8.4.0",
    "diff-match-patch": "^1.0.5",
    "marked": "^15.0.7",
    "monaco-editor": "^0.52.2",
    "ngx-markdown": "^19.1.0",
    "rxjs": "~7.8.0",
    "ts-md5": "^1.3.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.1",
    "@angular/cli": "^19.2.1",
    "@angular/compiler-cli": "^19.2.1",
    "@angular/localize": "^19.2.1",
    "@types/diff-match-patch": "^1.0.36",
    "@types/jasmine": "~5.1.7",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.2.0",
    "typescript": "~5.7.2"
  }
}
