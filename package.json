    "@myrmidon/cadmus-refs-geonames-lookup": "^9.0.0",
    "@myrmidon/cadmus-refs-historical-date": "9.0.1",
    "@myrmidon/cadmus-refs-lookup": "^9.0.1",
    "@myrmidon/cadmus-refs-proper-name": "10.0.1",
    "@myrmidon/cadmus-refs-viaf-lookup": "^9.0.0",
    "@myrmidon/cadmus-text-block-view": "^9.0.0",
    "@myrmidon/cadmus-text-ed": "^9.0.0",
    "@myrmidon/cadmus-text-ed-md": "^10.0.1",
    "@myrmidon/cadmus-text-ed-txt": "^9.0.0",
    "@myrmidon/cadmus-ui-flag-set": "^9.0.1",
    "@myrmidon/ngx-mat-tools": "^1.0.0",
    "@myrmidon/ngx-tools": "^2.0.0",
    "@myrmidon/paged-data-browsers": "^5.0.2",
    "3d-force-graph": "^1.78.2",
    "diff-match-patch": "^1.0.5",
    "echarts": "^5.6.0",
    "force-graph": "^1.50.1",
    "marked": "^15.0.12",
    "monaco-editor": "^0.52.2",
    "ngx-echarts": "^20.0.0",
    "ngx-markdown": "^20.0.0",
    "rxjs": "~7.8.0",
    "ts-md5": "^1.3.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
