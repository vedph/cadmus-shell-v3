    "@types/diff-match-patch": "^1.0.36",
    "@types/jasmine": "~5.1.8",
    "@types/three": "^0.178.1",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.0.0",
    "typescript": "~5.8.3"
  }
}
